# Stripe API Keys
# Get these from: https://dashboard.stripe.com/apikeys

# Public key (starts with pk_test_ or pk_live_)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_YOUR_PUBLISHABLE_KEY_HERE

# Secret key (starts with sk_test_ or sk_live_) - KEEP THIS SECRET!
STRIPE_SECRET_KEY=sk_test_YOUR_SECRET_KEY_HERE

# Site URL (for Stripe redirects and CORS)
SITE_URL=http://localhost:3000

# Financial Transparency Spreadsheet Edit Password
VITE_FINANCE_EDIT_PASSWORD=your_password_here

# Supabase Configuration
# Get these from: https://app.supabase.com/project/_/settings/api

# Supabase Project URL
SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_URL=https://your-project.supabase.co

# Supabase Keys
# ANON KEY - Safe to use in client-side code (read-only access)
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# SERVICE ROLE KEY - KEEP THIS SECRET! Never expose to client-side code
# Only use in Netlify Functions
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
